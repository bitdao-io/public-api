{
  "name": "public-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn openapi:yaml && yarn swagger",
    "dev": "next dev",
    "prebuild": "yarn openapi:yaml && yarn swagger",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openapi:yaml": "typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'types/openapi' 'types/*.ts'",
    "swagger": "yarn generate && next-swagger-doc-cli next-swagger-doc.json",
    "generate": "npx tsx ./scripts/swaggerconfig.js"
  },
  "dependencies": {
    "@stoplight/elements": "^7.7.10",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "alchemy-sdk": "^2.3.0",
    "autoprefixer": "^10.4.13",
    "eslint": "8.32.0",
    "eslint-config-next": "13.1.2",
    "ethers": "^5.7.2",
    "next": "13.1.2",
    "next-swagger-doc": "^0.3.6",
    "postcss": "^8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "subgraphql": "^0.0.0-rfc.14",
    "swagger-ui-react": "^4.15.5",
    "tailwindcss": "^3.2.4",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-react": "^4.11.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "cpy-cli": "^4.2.0",
    "eslint-config-prettier": "^8.6.0",
    "prettier": "^2.8.3",
    "ts-node": "^10.9.1",
    "typeconv": "^1.8.0"
  }
}
