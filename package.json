{
  "name": "public-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "yarn openapi:yaml && yarn swagger",
    "dev": "next dev",
    "prebuild": "yarn openapi:yaml && yarn swagger",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "openapi:yaml": "typeconv -f ts -t oapi --oapi-format yaml --output-extension 'swagger.yaml' --output-directory 'types/openapi' 'types/*.ts'",
    "swagger": "yarn generate && next-swagger-doc-cli next-swagger-doc.json",
    "generate": "npx tsx ./scripts/swaggerconfig.js"
  },
  "dependencies": {
    "@stoplight/elements": "^7.7.18",
    "@types/node": "20.4.2",
    "@types/react": "18.2.15",
    "@types/react-dom": "18.2.7",
    "alchemy-sdk": "^2.9.2",
    "autoprefixer": "^10.4.14",
    "csv-parse": "^5.3.6",
    "eslint": "8.45.0",
    "eslint-config-next": "13.4.10",
    "ethers": "^5.7.2",
    "graphql-request": "^6.1.0",
    "next": "13.4.10",
    "next-swagger-doc": "^0.4.0",
    "postcss": "^8.4.26",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "subgraphql": "^0.0.0-rfc.34",
    "swagger-ui-react": "^5.1.3",
    "tailwindcss": "^3.3.3",
    "typescript": "5.1.6",
    "viem": "^1.16.6"
  },
  "devDependencies": {
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-react": "^4.18.0",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "cpy-cli": "^5.0.0",
    "encoding": "^0.1.13",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typeconv": "^2.3.1"
  }
}
